<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressBarsViewS1">
        <attr name="color" format="color"/>
        <attr name="widthStroke" format="float"/>
    </declare-styleable>

    <declare-styleable name="RatingView">
        <attr name="numStars" format="integer"/>
        <attr name="rating" format="integer"/>
    </declare-styleable>

    <declare-styleable name="EmotionalFaceView">
        <attr name="faceColor" format="color" />
        <attr name="eyesColor" format="color" />
        <attr name="mouthColor" format="color" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="state" format="enum">
            <enum name="happy" value="0" />
            <enum name="sad" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ButtonView">
        <attr name="buttomIcon" format="reference"/>
        <attr name="buttonTitle" format="string"/>
        <attr name="buttonBg" format="reference"/>
    </declare-styleable>

</resources>